    def calculate(self):
        if self.operator == "+":
            self.display = str(self.a + self.b)
            self.label.setText(self.display)
        elif self.operator == "-":
            self.display = str(self.a - self.b)
            self.label.setText(self.display)
        elif self.operator == "x":
            self.display = str(self.a * self.b)
            self.label.setText(self.display)
        else:
            self.display = str(self.a / self.b)
            self.label.setText(self.display)

    def clear(self):
        self.a = 0
        self.b = 0
        self.operator = ""
        self.mousepress = 0
        self.label.setText("")
        self.display = ""


